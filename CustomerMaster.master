    .navbar-nav {
        width: 100%;
        justify-content: center;
        position: relative;
    }

    .nav-item {
        margin: 0 10px;
        position: relative;
    }

    .nav-link {
        display: flex;
        align-items: center;
        overflow: hidden;
        white-space: nowrap;
        transition: all 0.3s ease;
        color: white !important;
        font-size: 2rem;
        padding: 5px 12px;
    }

    .nav-icon {
        font-size: 2rem;
        transition: all 0.3s ease;
        margin-right: 6px;
        color: #FE5C36; /* Neon green */
    }

    .nav-link:hover .nav-icon {
        color: #085D94; /* Pink on hover */
        transform: scale(1.2);
    }

    .nav-text {
        opacity: 20;
        transform: translateX(10px);
        transition: all 0.3s ease;
        font-size: 1.1rem;
        color: black;
    }

    .nav-link:hover .nav-text {
        opacity: 1;
        transform: translateX(0);
    }

    .profile-icon {
        font-size: 2rem;
        color: #00bbf9; /* Cyan blue */
        transition: all 0.3s ease;
    }

    .nav-link:hover .profile-icon {
        color: #f15bb5;
        transform: scale(1.2);
    }

    .position-absolute.right-end {
        right: 0;
        top: 0;
    }

    @media (max-width: 991.98px) {
        .nav-text {
            opacity: 1 !important;
            transform: translateX(0) !important;
        }
    }
        /* Show dropdown on hover */
    .navbar-nav .dropdown:hover .dropdown-menu {
        display: block;
        margin-top: 0; /* optional: remove jump */
    }

    /* Optional: Add transition for dropdown */
    .dropdown-menu {
        transition: all 0.3s ease-in-out;
        border-radius: 0.5rem;
        box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
    }

    /* Smooth hover icon & text */
    .nav-link .nav-icon {
        transition: transform 0.2s ease, color 0.2s ease;
    }

    .nav-link:hover .nav-icon {
        transform: scale(1.1);
        color: #085D94;
    }

    .nav-link:hover .nav-text {
        color: black;
        margin-left: 4px;
    }


