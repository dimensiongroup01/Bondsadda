 /* Glowing effect base */
.glow-effect {
    background-color: #00558C !important;
    border-radius: 8px;
    color: inherit;
    transition: all 0.3s ease-in-out;
    animation: glow 1.8s infinite ease-in-out;
}

/* Glow animation */
@keyframes glow {
    0% {
        box-shadow: 0 0 4px #00558C;
    }
    50% {
        box-shadow: 0 0 15px #00bfff, 0 0 25px #00bfff;
    }
    100% {
        box-shadow: 0 0 4px #00558C;
    }
}

/* Default icon color */
.glow-effect .nav-icon {
    color: #FE5C36; /* example: original color (gold) */
    transition: color 0.3s ease-in-out;
}

/* On hover: icon and text become white */
.glow-effect:hover,
.glow-effect:hover .nav-text,
.glow-effect:hover .nav-icon {
    color: #ffffff !important;
}



