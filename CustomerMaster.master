        document.addEventListener("DOMContentLoaded", function () {
            var input = document.getElementById("txtResult");
            var searchBtn = document.getElementById("lnkSerch");

            if (input && searchBtn) {
                input.addEventListener("keypress", function (e) {
                    if (e.key === "Enter") {
                        e.preventDefault();
                        searchBtn.click();

                    }
                });
            }
        });

        document.addEventListener("DOMContentLoaded", function () {
            var input = document.getElementById("txtResult");
            var btn = document.getElementById("<%= lnkSerch.ClientID %>");

             if (input && btn) {
                 input.addEventListener("keypress", function (e) {
                     if (e.key === "Enter") {
                         e.preventDefault();
                         btn.click(); // Triggers ASP.NET search button
                     }
                 });
             }
         });

    </script>
